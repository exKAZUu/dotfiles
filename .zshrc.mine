# This loads RVM into a shell session.
[[ -s "$HOME/.rvm/scripts/rvm" ]] && . "$HOME/.rvm/scripts/rvm"

export PATH=$HOME/.nodebrew/current/bin:$PATH
nodebrew use latest

alias ga="git add -A"
alias gc="git commit -m"
alias gs="git status"
alias gb="git branch"
alias gpush="git push"
alias gpull="git pull"
